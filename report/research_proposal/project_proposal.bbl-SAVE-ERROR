      \strng{namehash}{c78b41c25d33a630d04bfdb1adbf6c63}
      \strng{fullhash}{c78b41c25d33a630d04bfdb1adbf6c63}
      \strng{bibnamehash}{c78b41c25d33a630d04bfdb1adbf6c63}
      \strng{authorbibnamehash}{c78b41c25d33a630d04bfdb1adbf6c63}
      \strng{authornamehash}{c78b41c25d33a630d04bfdb1adbf6c63}
